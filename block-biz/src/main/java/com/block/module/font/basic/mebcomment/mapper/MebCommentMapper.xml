<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<!--   mybatis的SQL配置文件  -->
<mapper namespace="MebCommentDao">

	<!-- 用于list结果集 -->
	<resultMap id="mebCommentMap" type="com.block.module.font.basic.mebcomment.entity.MebComment">
        	<result property="id" column="id"/>
        	<result property="userId" column="user_id"/>
        	<result property="columns" column="columns"/>
        	<result property="opinion" column="opinion"/>
        	<result property="createtime" column="createtime"/>
	</resultMap>
	
	<!-- 用于select查询公用抽取的列 -->
	<sql id="mebCommentColumns">
	    <![CDATA[
			id,user_id,columns,opinion,createtime
	    ]]>
	</sql>
</mapper>

