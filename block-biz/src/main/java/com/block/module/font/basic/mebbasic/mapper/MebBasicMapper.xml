<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<!--   mybatis的SQL配置文件  -->
<mapper namespace="MebBasicDao">

	<!-- 用于list结果集 -->
	<resultMap id="mebBasicMap" type="com.block.module.font.basic.mebbasic.entity.MebBasic">
        	<result property="id" column="id"/>
        	<result property="username" column="username"/>
        	<result property="createtime" column="createtime"/>
        	<result property="updatetime" column="updatetime"/>
        	<result property="referer" column="referer"/>
        	<result property="status" column="status"/>
        	<result property="type" column="type"/>
        	<result property="nickname" column="nickname"/>
        	<result property="qq" column="qq"/>
        	<result property="weixin" column="weixin"/>
        	<result property="phone" column="phone"/>
        	<result property="address" column="address"/>
        	<result property="longitude" column="longitude"/>
        	<result property="latitude" column="latitude"/>
        	<result property="images" column="images"/>
        	<result property="credit" column="credit"/>
        	<result property="opinion" column="opinion"/>
        	<result property="times" column="times"/>
        	<result property="profile" column="profile"/>
	</resultMap>
	
	<!-- 用于select查询公用抽取的列 -->
	<sql id="mebBasicColumns">
	    <![CDATA[
			id,username,createtime,updatetime,referer,status,type,nickname,qq,weixin,phone,address,longitude,latitude,images,credit,opinion,times,profile
	    ]]>
	</sql>
</mapper>

